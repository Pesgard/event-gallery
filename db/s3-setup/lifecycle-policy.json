{
  "Rules": [
    {
      "Id": "DeleteOldThumbnails",
      "Status": "Enabled",
      "Filter": {
        "Prefix": "events/"
      },
      "Expiration": {
        "Days": 90
      },
      "NoncurrentVersionExpiration": {
        "NoncurrentDays": 30
      }
    },
    {
      "Id": "ArchiveOldImages",
      "Status": "Enabled",
      "Filter": {
        "Prefix": "events/"
      },
      "Transitions": [
        {
          "Days": 365,
          "StorageClass": "STANDARD_IA"
        },
        {
          "Days": 730,
          "StorageClass": "GLACIER"
        }
      ]
    }
  ]
}

